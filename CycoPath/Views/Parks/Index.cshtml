@model IEnumerable<CycoPath.Models.Park>

    @{
    ViewBag.Title = "Index";
    }



    <iframe width="100%" height="400" src="https://data.gov.sg/dataset/park-connector-loops/resource/1029bf7a-f702-4bbf-929f-4bf2e18c5f61/view/b9812887-bc94-4fa8-9a38-09a243a67828" frameBorder="0"></iframe>



    @using (Html.BeginForm("ParkResult", "Parks", FormMethod.Post))
    {
    @Html.AntiForgeryToken()
    <strong>Start</strong>
    <div class="form-group">
        <select class="form-control" name="start">
            @foreach (var item in Model)
            {
            <option>
                @Html.DisplayFor(modelItem => item.Name)
            </option>

            }
        </select>
    </div>
    <strong>End</strong>
    <div class="form-group">
        <select class="form-control" name="end">
            @foreach (var item in Model)
            {

            <option>
                @Html.DisplayFor(modelItem => item.Name)
            </option>

            }
        </select>
    </div>
    <button type="submit" class="btn btn-default">
        Submit
    </button>

    }



